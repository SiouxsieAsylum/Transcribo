{
    "extends":[
        "eslint:recommended",
        "prettier"
    ],
    "rules": {
        "eqeqeq": "error",
        "no-undef": "off"
    },
    "ignorePatterns": ["**.config.js", "node_modules/**"],
    "env": {
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 2017
    }
}